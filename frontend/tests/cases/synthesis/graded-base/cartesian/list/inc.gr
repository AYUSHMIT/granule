language GradedBase

data List a = Cons a (List a) | Nil

data N = S N | Z

map : (List N) %Any -> (N %Any -> N) %Any -> (List N)
map Nil f = Nil; 
map (Cons x xs) f = (Cons (f x) (map xs f))

inc : forall a . (List N) %Any -> (List N)
spec
    inc Nil = Nil;
    inc (Cons (S Z) Nil) = (Cons (S (S Z)) Nil);
    map %Any
inc = ?