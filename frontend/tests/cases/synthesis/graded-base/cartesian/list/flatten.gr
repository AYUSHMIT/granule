language GradedBase 

data List a = Cons a (List a) | Nil

append : forall {a : Type} . (List a) %Any -> (List a) %Any ->  List a
append Nil y = y;
append (Cons z u) y = (Cons z) ((append u) y)

flatten : forall a . (List (List a)) %Any -> (List a)
spec 
    flatten (Cons (Cons 1 Nil) (Cons (Cons 1 Nil) Nil)) = Cons 1 (Cons 1 Nil);
    flatten %Any, append %Any
flatten = ?