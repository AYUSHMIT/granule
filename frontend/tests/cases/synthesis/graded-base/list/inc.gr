language GradedBase

data List a = Cons a (List a) | Nil

data N = S N | Z

map : (List N) %1..∞ -> (N %1..∞ -> N) %0..∞ -> (List N)
map Nil f = Nil; 
map (Cons x xs) f = (Cons (f x) (map xs f))

inc : forall a . (List N) %1..∞ -> (List N)
spec
    inc Nil = Nil;
    inc (Cons (S Z) Nil) = (Cons (S (S Z)) Nil);
    map %1..∞
inc = ?