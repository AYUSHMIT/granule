language GradedBase

data Bool = True | False

and : Bool %0..∞ -> Bool %0..∞ -> Bool
spec
    and True True = True;
    and False True = False;
    and True False = False;
    and False False = False;
and x y = 
    (case y of
      True -> 
    (case x of
      True -> y;
      False -> x);
      False -> 
    (case x of
      True -> y;
      False -> y))