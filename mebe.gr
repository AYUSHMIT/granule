
data Maybe a = Nothing | Just a
data Bool = True | False

isSome : forall {a : Type} . Maybe (a [0]) -> Bool
isSome Nothing = False;
isSome (Just [_]) = True

not : Bool -> Bool
not False = True;
not True = False

--- Whether a `Maybe a` value is `Nothing`
isNone : forall {a : Type} . Maybe (a [0]) -> Bool
isNone m = not (isSome m)
