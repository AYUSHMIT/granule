import File

main : FileIO String
main =
    let |mh|    = |(openFile "LICENSE" ReadMode)| in
    let h       <- mh;
        h'      <- mh;
        ()      <- hClose h';
        (h, c)  <- hGetChar h;
        (h, c') <- hGetChar h;
        ()      <- hClose h
    in pure ((showChar c) `stringAppend` (showChar c'))
