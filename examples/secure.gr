-- The following is not typeable
--leak : |Int| Hi -> |Int| Lo
--leak |x| = |x|

notALeak : ||Int| Hi| 0 -> |Int| Lo
notALeak |x| = |0|

try : forall (l : Level) . |Int| l -> |Int| l
try |x| = |(x + x)|

go : |Int| Hi -> |Int| Hi
go |x| = let |z : Int| Level = try |x| in |(x + z)|

main : |Int| Hi
main = |1|
