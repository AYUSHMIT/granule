id : forall (a : Type) . a -> a
id x = x

idAlt : forall (a : Type) . a -> a
idAlt = \(x : a) -> x

-- Not allowed
-- idBad : forall (a : Type) . a -> a
-- idBad = \(x : Int) -> x

fst : forall (a : Type, b : Type) . (a, b |0|) -> a
fst (x, |_|) = x

snd : forall (a : Type, b : Type) . (a |0|, b) -> b
snd (|_|, y) = y

main : Int
main = fst (snd (|1|, (2, |3|)))
