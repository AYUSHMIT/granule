foo : List 3 Int
foo = Cons 1 (Cons 2 (Cons 3 Nil))

isEmpty : forall (n : Nat) . List n Int -> Bool
isEmpty xs =
  case xs of
    Nil        -> True;
    (Cons _ _) -> False

map : forall (n : Nat) . (Int -> Int) |n| -> List n Int -> List n Int
map |f| ys =
  case ys of
    Nil -> Nil;
    (Cons x xs) -> Cons (f x) (map |f| xs)

main : List 3 Int
main = map |(\(x : Int) -> x * 2)| foo