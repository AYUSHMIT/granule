-- Small example of case expressions in Granule

#include "StdLib/Prelude.gr"

constId : Int -> Int
constId x = case 0 of 0 -> x

constId2 : Int -> Int
constId2 x =
  case 0 of
    0 -> x;
    1 -> x

add : Int |1| -> Int |1| -> Int
add |x| |y| = x+y

main : Int
main = add (if0 |0| |1| |2|) (if0 |1| |1| |2|)
